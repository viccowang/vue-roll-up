{
  "name": "vue-roll-up",
  "version": "0.1.5",
  "private": false,
  "keywords": [
    "vue",
    "vue2",
    "marquee",
    "roll",
    "scroll",
    "up"
  ],
  "license": "MIT",
  "description": "A vertical marquee component for vue 2.x.",
  "author": "Vicco Wang <viccowang@hotmail.com> (https://blog.vicco)",
  "main": "dist/vue-roll-up.common.js",
  "repository": "https://github.com/viccowang/vue-roll-up.git",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name vue-roll-up ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.8.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "vue": "^2.6.10"
  }
}
